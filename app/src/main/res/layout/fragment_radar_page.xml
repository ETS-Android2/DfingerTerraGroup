<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.iamliterallymalding.radarPageFrag">

    <Button
        android:id="@+id/RadarPageButton"
        android:layout_width="139dp"
        android:layout_height="82dp"
        android:layout_marginStart="136dp"
        android:text="@string/home"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="561dp" />

    <ImageView
        android:id="@+id/radarView"
        android:layout_width="381dp"
        android:layout_height="495dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="29dp"
        android:layout_marginEnd="14dp"
        android:layout_marginBottom="37dp"
        android:src="@mipmap/radar_foreground"
        app:layout_constraintBottom_toTopOf="@+id/RadarPageButton"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@mipmap/radar_foreground" />

</androidx.constraintlayout.widget.ConstraintLayout>